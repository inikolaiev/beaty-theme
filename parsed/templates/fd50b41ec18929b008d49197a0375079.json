{
  "components/common/breadcrumbs": "<ul class=\"breadcrumbs\">\n    {{#each breadcrumbs}}\n        {{#if url}}\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\">\n                {{#if @index '>' 0}}\n                    <svg class=\"icon\"><use xlink:href=\"#icon-ih-arrow-right\"></use></svg>\n                {{/if}}\n                <a data-instantload href=\"{{url}}\" class=\"breadcrumb-label\">\n                    {{#if @index '===' 0}}\n                        <svg class=\"icon\"><use xlink:href=\"#icon-ih-small-home\"></use></svg>\n                    {{/if}}\n                    <span>{{name}}</span>\n                </a>\n            </li>\n        {{else}}\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-ih-arrow-right\"></use></svg>\n                <span class=\"breadcrumb-label\">{{name}}</span>\n            </li>\n        {{/if}}\n    {{/each}}\n</ul>\n\n<script type=\"application/ld+json\">\n{\n    \"@context\": \"https://schema.org\",\n    \"@type\": \"BreadcrumbList\",\n    \"itemListElement\":\n    [\n        {{#each breadcrumbs}}\n        {\n            \"@type\": \"ListItem\",\n            \"position\": {{add @index 1}},\n            \"item\": {\n                \"@id\": {{{JSONstringify url}}},\n                \"name\": {{{JSONstringify name}}}\n            }\n        }{{#unless @last}},{{/unless}}\n        {{/each}}\n    ]\n}\n</script>"
}